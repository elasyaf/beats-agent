---
filebeat.prospectors:
  - type: log
    enabled: true
    paths:
      - /var/log/*.log
      - /var/log/*/*.log

output:
  elasticsearch:
    hosts: ["{{elasticsearch_hosts}}:9200"]

setup:
  kibana:
    host: "{{kibana_hosts}}:5601"

setup.template.name: "filebeat"
setup.template.fields: "fields.yml"
setup.template.overwrite: true
