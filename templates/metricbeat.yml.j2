---
metricbeat.modules:
- module: system
  metricsets:
    - cpu
    - filesystem
    - memory
    - network
    - process
  enabled: true
  period: 10s
  processes: ['.*']
  cpu_ticks: false

output.elasticsearch:
  hosts: ["{{elasticsearch_hosts}}:9200"]

setup.kibana:
  host: "{{kibana_hosts}}:5601"

setup.template.overwrite: true
setup.template.enabled: false
